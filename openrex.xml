<?xml version="1.0" encoding="UTF-8"?>
<manifest>
	<default sync-j="4" revision="master"/>
	<remote fetch="https://git.yoctoproject.org/git" name="yocto"/>
	<remote fetch="https://github.com/Freescale" name="freescale"/>
	<remote fetch="https://github.com/openembedded" name="oe"/>
	<remote fetch="https://github.com/Ghalgaoui" name="openrex"/>

	<project remote="yocto" revision="kirkstone" name="poky" path="sources/poky"/>
	<project remote="oe" revision="kirkstone" name="meta-openembedded" path="sources/meta-openembedded"/>
	<project remote="yocto" revision="kirkstone" name="meta-freescale" path="sources/meta-freescale"/>
	<project remote="freescale" revision="kirkstone" name="meta-freescale-3rdparty" path="sources/meta-freescale-3rdparty"/>
	<project remote="freescale" revision="kirkstone" name="meta-freescale-distro" path="sources/meta-freescale-distro"/>
	<project remote="freescale" revision="kirkstone" name="Documentation" path="sources/Documentation"/>

	<project remote="openrex" name="meta-openrex" revision="kirkstone" path="sources/meta-openrex">
		<linkfile dest="README" src="README"/>
		<linkfile dest="setup-environment" src="setup-environment"/>
	</project>

	<project remote="openrex" name="meta-bsp-openrex" revision="kirkstone" path="sources/meta-bsp-openrex"/>
</manifest>
